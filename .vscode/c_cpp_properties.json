{
    "version": 4,
    "env": {
        "mtbCompilerArgs": [
            "-c",
            "-mcpu=cortex-m4",
            "--specs=nano.specs",
            "-Og",
            "-mfloat-abi=softfp",
            "-mfpu=fpv4-sp-d16",
            "-mthumb",
            "-ffunction-sections",
            "-fdata-sections",
            "-ffat-lto-objects",
            "-g",
            "-Wall"
        ],
        "mtbDefaultIncludePath": [
            ".",
            "./src",
            "../mtb_shared/capsense/release-v2.10.0",
            "../mtb_shared/core-lib/release-v1.4.0",
            "../mtb_shared/core-lib/release-v1.4.0/include",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/COMPONENT_PSOC6HAL",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/COMPONENT_PSOC6HAL/COMPONENT_CAT1A",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/COMPONENT_PSOC6HAL/COMPONENT_CAT1A/include",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/COMPONENT_PSOC6HAL/COMPONENT_CAT1A/include/pin_packages",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/COMPONENT_PSOC6HAL/COMPONENT_CAT1A/include/triggers",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/COMPONENT_PSOC6HAL/include",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/COMPONENT_PSOC6HAL/source",
            "../mtb_shared/mtb-hal-cat1/release-v1.6.0/include",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/cmsis",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/cmsis/include",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/devices",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/devices/COMPONENT_CAT1A",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/devices/COMPONENT_CAT1A/include",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/devices/COMPONENT_CAT1A/include/ip",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/drivers",
            "../mtb_shared/mtb-pdl-cat1/release-v2.4.1/drivers/include",
            "${config:modustoolbox.toolsPath}/gcc/lib/gcc/arm-none-eabi/10.3.1/include",
            "${config:modustoolbox.toolsPath}/gcc/lib/gcc/arm-none-eabi/10.3.1/include-fixed",
            "${config:modustoolbox.toolsPath}/gcc/arm-none-eabi/include",
            "${config:modustoolbox.toolsPath}/gcc/arm-none-eabi/include/c++/10.3.1",
            "${config:modustoolbox.toolsPath}/gcc/arm-none-eabi/include/c++/10.3.1/arm-none-eabi/thumb/v7e-m/fpv4-sp/softfp",
            "${config:modustoolbox.toolsPath}/gcc/arm-none-eabi/include/c++/10.3.1/backward"
        ],
        "mtbDefaultDefines": [
            "CY_USING_HAL",
            "CY_APPNAME_mtb_example_psoc6_spi_master_dma",
            "CY8C624ABZI_S2D44",
            "CY_TARGET_DEVICE=CY8C624ABZI_S2D44",
            "TARGET_CY8CPROTO_062_4343W",
            "CY_TARGET_BOARD=CY8CPROTO_062_4343W",
            "COMPONENT_4343W",
            "COMPONENT_CAT1",
            "COMPONENT_CAT1A",
            "COMPONENT_CM0P_SLEEP",
            "COMPONENT_CM4",
            "COMPONENT_CUSTOM_DESIGN_MODUS",
            "COMPONENT_CY8CPROTO_062_4343W",
            "COMPONENT_HCI_UART",
            "COMPONENT_MURATA_1DX",
            "COMPONENT_PSOC6HAL",
            "COMPONENT_SOFTFP",
            "DEBUG",
            "CY_SUPPORTS_DEVICE_VALIDATION",
            "__GNUC__"
        ]
    },
    "configurations": [
        {
            "name": "MTB",
            "compilerPath": "${config:modustoolbox.toolsPath}/gcc/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "${mtbCompilerArgs}"
            ],
            "compileCommands": "./build/compile_commands.json",
            "intelliSenseMode": "gcc-x86",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "includePath": [
                "${mtbDefaultIncludePath}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "defines": [
                "${mtbDefaultDefines}"
            ]
        }
    ]
}
